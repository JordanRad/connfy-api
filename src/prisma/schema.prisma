// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./connfy.db"
}
model User {
  id      Int      @default(autoincrement()) @id
  email   String   @unique
  name    String 
}
model Meeting {
  id Int @default(autoincrement()) @id
  title String @unique
  noteId Int
  userId Int 
  date DateTime @default(now())
}
generator client {
  provider = "prisma-client-js"
}
